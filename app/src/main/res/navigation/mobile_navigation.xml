<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_browse">

    <fragment
        android:id="@+id/navigation_browse"
        android:name="com.example.gittest.ui.browse.BrowseFragment"
        android:label="@string/title_browse"
        tools:layout="@layout/fragment_browse" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.gittest.ui.cart.CartFragment"
        android:label="@string/title_cart"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.example.gittest.ui.orders.OrderFragment"
        android:label="@string/title_orders"
        tools:layout="@layout/fragment_order" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.gittest.ui.account.AccountFragment2"
        android:label="@string/title_account"
        tools:layout="@layout/fragment_account" />
</navigation>